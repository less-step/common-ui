	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest",
			"plugin:storybook/recommended"
		]
	},
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.{js,jsx,ts,tsx}",
			"!src/**/*.d.ts"
		],
		"moduleFileExtensions": [
			"web.js",
			"js",
			"web.ts",
			"ts",
			"web.tsx",
			"tsx",
			"json",
			"web.jsx",
			"jsx",
			"node"
		],
		"moduleNameMapper": {
			"^.+\\.(css|less|scss)$": "identity-obj-proxy",
			"^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
			"^react-native$": "react-native-web",
			"^axios$": "axios/dist/axios.js",
			"^lodash-es$": "lodash-es/lodash.js"
		},
		"modulePaths": [],
		"resetMocks": true,
		"roots": [
			"<rootDir>/src"
		],
		"setupFiles": [
			"react-app-polyfill/jsdom"
		],
		"setupFilesAfterEnv": [
			"<rootDir>/src/setupTests.ts"
		],
		"testEnvironment": "jsdom",
		"testMatch": [
			"<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
			"<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
		],
		"transform": {
			"^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js",
			"^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
			"^.+\\.css$": "<rootDir>/config/jest/cssTransform.js"
		},
		"watchPlugins": [
			"jest-watch-typeahead/filename",
			"jest-watch-typeahead/testname"
		],
		"transformIgnorePatterns": [
			"[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
			"^.+\\.module\\.(css|sass|scss)$",
			"/node_modules/(?!(axios)/)",
			"/node_modules/(?!(lodash-es)/)"
		]
	},
	"name": "less-step-common",
	"main": "dist/component.es.js",
	"module": "dist/component.es.js",
	"types": "dist/component.d.ts",
	"files": [
		"dist"
	],
	"private": false,
	"scripts": {
		"build": "node scripts/build.js",
		"build-storybook": "storybook build",
		"clean:dist": "rimraf dist",
		"lint": "eslint --ext js,jsx,ts,tsx src",
		"build-es": "npm run clean:dist && rollup --config rollup.config.js",
		"build-umd": "rollup --config rollup.umd.config.js",
		"build-ts": "tsc -p tsconfig.build.json",
		"build-sass": "node-sass src/styles/index.scss dist/component.css",
		"build-component": "npm run clean:dist && npm run build-es && npm run build-umd",
		"start": "node scripts/start.js",
		"storybook": "storybook dev -p 6006",
		"test:watch": "node scripts/test.js",
		"test": "cross-env CI=true node scripts/test.js",
		"prepublishOnly": "npm run lint && npm run test && npm run build-component",
		"release": "npm publish",
		"prepare": "husky install"
	}
}
